    "testFileExtensions": [
      "es6",
      "js"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "es6"
    ],
    "unmockedModulePathPatterns": [
      "react",
      "react-addons-test-utils",
      "react-native-router-flux",
      "promise",
      "source-map",
      "key-mirror",
      "immutable",
      "fetch",
      "redux",
      "redux-thunk",
      "fbjs"
    ],
    "collectCoverage": true,
    "verbose": true,
      "/^[./a-zA-Z0-9$_-]+.jpg$/": "RelativeImageStub"
    },
    "resolver": null
  },
  "testFileExtensions": [
    "es6",
    "js"
  ],
  "moduleFileExtensions": [
    "js",
    "json",
    "es6"
  ],
  "unmockedModulePathPatterns": [
    "react",
    "react-addons-test-utils",
    "react-native-router-flux",
    "promise",
    "source-map",
    "key-mirror",
    "immutable",
    "fetch",
    "redux",
    "redux-thunk",
    "fbjs"
  ],
  "collectCoverage": true,
  "verbose": true,
  "private": true
}
