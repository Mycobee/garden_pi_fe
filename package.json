{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "test": "jest",
    "test:update": "jest --verbose --coverage --updateSnapshot"
  },
  "dependencies": {
    "@unimodules/react-native-adapter": "^3.0.0",
    "enzyme": "^3.10.0",
    "expo": "^34.0.1",
    "expo-camera": "^6.0.0",
    "expo-cli": "^3.0.10",
    "expo-image-picker": "^6.0.0",
    "google-maps-react": "^2.0.2",
    "jest": "^24.9.0",
    "path": "^0.12.7",
    "react": "16.8.3",
    "react-dom": "^16.8.6",
    "react-native": "https://github.com/expo/react-native/archive/sdk-34.0.0.tar.gz",
    "react-native-chart-kit": "^3.3.1",
    "react-native-dotenv": "^0.2.0",
    "react-native-gesture-handler": "~1.3.0",
    "react-native-maps": "^0.25.0",
    "react-native-progress-circle": "^2.0.1",
    "react-native-svg": "^9.8.4",
    "react-native-web": "^0.11.4",
    "react-navigation": "^4.0.0",
    "react-navigation-stack": "^1.5.1"
  },
  "devDependencies": {
    "babel-jest": "^24.9.0",
    "babel-preset-expo": "^6.0.0",
    "babel-preset-react-native": "^4.0.1",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.4.0",
    "jest": "^24.9.0",
    "jest-react-native": "^18.0.0",
    "react-test-renderer": "^16.8.6"
  },
  "jest": {
    "preset": "jest-react-native",
    "transf": "<rootDir>/node_modules/babel-jest",
    "transformIgnorePatterns": [
      "node_modules/(?!(react-native",
      "|expo-permissions",
      "|react-native-progress-circle",
      "|react-native-chart-kit",
      "|react-native-svg",
      "|@unimodules",
      "|expo-camera",
      ")/)"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "moduleNameMapper": {
      "/^[./a-zA-Z0-9$_-]+.jpg$/": "RelativeImageStub",
      "\\.(jpg|ico|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga|tsx)$": "<rootDir>/fileMock.js",
      "\\.(css|less)$": "<rootDir>/fileMock.js"
    },
    "resolver": null
  },
  "testFileExtensions": [
    "es6",
    "js"
  ],
  "moduleFileExtensions": [
    "js",
    "json",
    "es6"
  ],
  "unmockedModulePathPatterns": [
    "react",
    "react-addons-test-utils",
    "react-native-router-flux",
    "promise",
    "source-map",
    "key-mirror",
    "immutable",
    "fetch",
    "redux",
    "redux-thunk",
    "fbjs"
  ],
  "collectCoverage": true,
  "verbose": true,
  "private": true
}
